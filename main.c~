#include <stdio.h>
# include <string.h>
# include <time.h>
# include <stdlib.h>
# include <math.h>
# include <unistd.h>
#include <queue>
#include "modjsg.h"
#include "global.h"

queue<gridlet> qu[100];
int main(int argc, char* argv[]){

int choice, block_size,popsize, ngen, init_seed;
    if (argc< 5)
    {
        printf("\n Usage ./nsga2r GNUPLOT<1:YES 0: NO>  job_block_size   pop_size  number_of_generation seed<any unsigned integer>\n");
       return 1;
	}
	
	choice = atoi(argv[1]);
    block_size = atoi(argv[2]);
    popsize = atoi(argv[3]);

    ngen = atoi(argv[4]);
    init_seed = atoi(argv[5]);
    
    modjsg(choice, block_size, popsize,ngen, init_seed);
    
    return 0;

}
